<% @presets.each do |preset| %>
  <div class="d-flex p-4 justify-content-around">
    <%= link_to preset_path(preset) do %>
      <%= image_tag preset.file.variant(:thumb) %>
    <%- end %>
    <%= preset.title %>
    <div class="d-flex">
      <%= preset.price %>
      <%= button_to "Add to wallet" %>
    </div>
  </div>
<% end %>
